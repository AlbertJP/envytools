<?xml version="1.0" encoding="UTF-8"?>
<database xmlns="http://nouveau.freedesktop.org/"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://nouveau.freedesktop.org/ rules-ng.xsd">

<import file="nvchipsets.xml"/>
<import file="nvclasses.xml"/>

<domain name="NV01_SUBCHAN" prefix="none" size="0x2000">
	<stripe name="SUBCHAN" prefix="chipset">
		<reg32 offset="0x00" name="OBJECT" type="object" />
		<stripe variants="NV50-">
			<reg32 offset="0x10" name="FENCE_ADDRESS_HIGH" />
			<reg32 offset="0x14" name="FENCE_ADDRESS_LOW" />
			<reg32 offset="0x18" name="FENCE_COUNTER" />
			<reg32 offset="0x1c" name="FENCE_GET" />
		</stripe>
		<stripe variants="NV10-">
			<reg32 offset="0x50" name="REF_CNT" />
			<reg32 offset="0x60" name="DMA_SEMAPHORE" type="object" />
			<reg32 offset="0x64" name="SEMAPHORE_OFFSET" />
			<reg32 offset="0x68" name="SEMAPHORE_ACQUIRE" />
			<reg32 offset="0x6c" name="SEMAPHORE_RELEASE" />
		</stripe>
	</stripe>
	<stripe prefix="obj-class">
		<import file="nv_m2mf.xml" />
		<stripe variants="NV04_MEMORY_TO_MEMORY_FORMAT-NV50_MEMORY_TO_MEMORY_FORMAT">
			<use-group name="m2mf"/>
		</stripe>
	</stripe>
</domain>

</database>
