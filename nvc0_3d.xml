	<value value="0x12" name="32"/>
	<value value="0x13" name="8_8_8"/>
	<value value="0x18" name="8_8"/>
	<value value="0x1b" name="16"/>
	<value value="0x1d" name="8"/>
	<value value="0x30" name="2_10_10_10"/>
</enum>

<enum name="nvc0_msaa_mode" inline="yes">
